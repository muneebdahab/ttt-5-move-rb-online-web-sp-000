#!/usr/bin/env ruby

require_relative '../lib/move.rb'
puts "Welcome to Tic Tac Toe!"
board = [" ", " ", " ", " ", " ", " ", " ", " ", " "]
puts "Where would you like to go?"
user_input = gets.strip
input_to_index(user_input)
display_board(board, input_to_index, character = "X")
def board
begin(board)
 old_stdout = $stdout
    $stdout = StringIO.new('','w')
    yield
    $stdout.string
  ensure
    $stdout = old_stdout
  end
end